{
  "name": "fullstackwebdevelopment",
  "version": "1.0.0",
  "description": "\"Node and GIT\"",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "imagemin": "imagemin img/* --out-dir='dist/img'",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\"",
    "usemin": "usemin contactus.html -d dist --htmlmin -o dist/contactus.html && usemin aboutus.html -d dist --htmlmin -o dist/aboutus.html && usemin index.html -d dist --htmlmin -o dist/index.html",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://SagarKonda@bitbucket.org/SagarKonda/bootstrap-course.git"
  },
  "author": "\"Sagar Konda\"",
  "license": "ISC",
  "homepage": "https://bitbucket.org/SagarKonda/bootstrap-course#readme",
  "devDependencies": {
    "cssmin": "^0.4.3",
    "grunt": "^1.0.2",
    "grunt-sass": "^3.1.0",
    "htmlmin": "0.0.7",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.5.4",
    "node-sass": "^4.13.1",
    "onchange": "^3.3.0",
    "parallelshell": "github:darkguy2008/parallelshell#8fd83e2",
    "rimraf": "^2.6.2",
    "time-grunt": "^1.4.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9"
  }
}
